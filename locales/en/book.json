{
  "title": "{{room-name}} - Bookings",
  "heading": "{{-start-span}}Bookings{{-end-span}} {{room-name}}",
  "dateForm": {
    "date": {
      "legend": "Date",
      "errors": {
        "presence": "Put in a date",
        "format": "This is not a date"
      }
    },
    "submit": "Change date"
  },
  "bookForm": {
    "heading": "Book {{room-name}}",
    "description": {
      "label": "Reason for booking",
      "errors": {
        "presence": "Need a reason"
      }
    },
    "name": {
      "label": "Full name",
      "errors": {
        "presence": "Need a name"
      }
    },
    "from": {
      "legend": "From",
      "errors": {
        "hours": "Not good hours",
        "mintues": "Not good minutes",
        "backwards": "From can't be greater than until",
        "same": "From time can't be the same as util time!",
        "double": "Can't double book!"
      }
    },
    "until": {
      "legend": "Until",
      "errors": {
        "hours": "Not good hours",
        "mintues": "Not good minutes",
        "backwards": "Until can't be less than from",
        "same": "Until time can't be the same as from time!",
        "double": "Can't double book!"
      }
    },
    "submit": "Book"
  }
}
